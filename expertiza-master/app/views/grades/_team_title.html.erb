<%if !@avg_of_avg.nan?%>
<%= render :partial => 'team_charts', :locals => {:prefix => nil} %>
<%end%>
<TR class="team" <% if prefix %>id='<%= prefix %>_<%= num + 4 %>' style="display:none"<% end %>>
	<th style="text-align:center;">&nbsp;</th>
	<th style="text-align:center;" COLSPAN="<%= col_outer_th_span_value %>">Submitted work</th>
	<th style="text-align:center;" WIDTH="9">&nbsp;</th>
</TR>

<TR class="team" <% if prefix %>id='<%= prefix %>_<%= num + 5 %>' style="display:none"<% end %>>
	<th style="text-align:left;">Team</th>
	<th style="text-align:center;" COLSPAN="<%= col_avg_th_span_value %>">Average</th>
	<th style="text-align:center;" COLSPAN="<%= col_range_th_span_value %>">Range</th>
	<th style="text-align:center;" WIDTH="9">&nbsp;</th>
</TR>
