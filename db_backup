-- Table structure for table `deadline_types`
--

DROP TABLE IF EXISTS `deadline_types`;
CREATE TABLE `deadline_types` (
  `id` int(11) NOT NULL auto_increment,
  `name` varchar(32) default NULL,
  PRIMARY KEY  (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

