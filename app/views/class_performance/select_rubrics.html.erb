<h1> Class Performance Rubric</h1>
<h4> Select any number of rubric criterias from the following </h4>

<%= form_for :class_performance , :url => { :controller => "class_performance", :action =>"show_class_performance", :id => params[:id] } do |f| %>
  <%= f.select :criterias, options_for_select(@results.map { |obj| [obj['txt'], obj['id']] }),  {} ,{ :multiple => true , :size => 10, :required => true } %>
  <%= f.submit "Show Performance" %>
<% end %>
