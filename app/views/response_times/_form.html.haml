= form_for @response_time do |f|
  - if @response_time.errors.any?
    #error_explanation
      %h2= "#{pluralize(@response_time.errors.count, "error")} prohibited this response_time from being saved:"
      %ul
        - @response_time.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :map_id
    = f.number_field :map_id
  .field
    = f.label :link
    = f.text_field :link
  .field
    = f.label :round
    = f.number_field :round
  .field
    = f.label :start_at
    = f.datetime_select :start_at
  .field
    = f.label :end_at
    = f.datetime_select :end_at
  .actions
    = f.submit 'Save'