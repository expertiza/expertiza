<div>
  <% if is_available(session[:user],owner_id) || (session[:user].role_id == 6 && is_available(User.find(Ta.get_my_instructor(session[:user].id)),owner_id))%>
    <%= link_to 'Edit ' + model.downcase, { controller: controller, action: :edit, id: node.node_object.id }, class: "btn btn-default" %>
    <%= link_to 'Delete ' + model.downcase, { controller: controller, action: 'delete', id: node.node_object.id }, class: "btn btn-default" %>
    <% if Object.const_get(model).find(node.node_object_id).private %>
      <%= link_to 'Make public', { :controller => controller, :action=>'toggle_access', :id => node.node_object_id }, class: "btn btn-default" %>
    <% else %>
      <%= link_to 'Make private', { :controller => controller, :action=>'toggle_access', :id => node.node_object_id}, class: "btn btn-default" %>
    <% end %>
  <% end %>
  <%= link_to 'Copy ' + model.downcase, {assets: controller, :controller => controller, id: node.node_object}, class: "btn btn-default" %>
</div>