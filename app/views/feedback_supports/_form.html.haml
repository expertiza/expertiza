= form_for @feedback_support do |f|
  - if @feedback_support.errors.any?
    #error_explanation
      %h2= "#{pluralize(@feedback_support.errors.count, "error")} prohibited this feedback_support from being saved:"
      %ul
        - @feedback_support.errors.full_messages.each do |msg|
          %li= msg
.field
  = f.label :user_id
  = f.number_field :user_id

.field
  = f.label :title
  = f.text_field :title

  .field
    = f.label :description
    = f.text_field :description


  .actions
    = f.submit 'Save'
