<div id="rating_div">
  <table width="100%" style= "border-collapse: collapse;" >
    <% if @search_results %>
      <% for search_result in @search_results %>
        <% row_color = cycle("CCCCCC", "FFFFFF")%>
        <tr style="background: #<%= row_color %>;" >
          <td> <%= render :partial => 'result', :locals => {:search_result => search_result, :order_by => @order_by, :called_function => @method_name } %> </td>
        </tr>
      <% end %>
    <% end %>
  </table>
</div>



