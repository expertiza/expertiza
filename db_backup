  CONSTRAINT `fk_survey_questionnaires` FOREIGN KEY (`survey_id`) REFERENCES `questionnaires` (`id`),
  CONSTRAINT `fk_survey_questions` FOREIGN KEY (`question_id`) REFERENCES `questions` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

