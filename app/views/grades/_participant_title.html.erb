<% if has_team_and_metareview?[:true_num] == 2 %>
	<!--100 = 19 + 8 * 9 + 9-->
	<% namecolwidth = 16 %>
	<% colwidth = 9 %>
<% elsif has_team_and_metareview?[:true_num] == 1 %>
	<!--100 = 16 + 6 * 12 + 12-->
	<% namecolwidth = 16 %>
	<% colwidth = 9 %>
<% else %>
	<!--100 = 25 + 4 * 15 + 15-->
	<% namecolwidth = 16 %>
	<% colwidth = 9 %>
<% end %>

<TR <% if prefix %>id='<%= prefix %>_1' style="display:none; background-color: white;"<% end %>>
<th style="text-align:center;" WIDTH="<%= namecolwidth %>%">&nbsp;</th>
<th style="text-align:center;" COLSPAN="2" WIDTH="<%= colwidth*2 %>%">Submitted work</th>
<% if has_team_and_metareview?[:has_metareview] %>
	<th style="text-align:center;" COLSPAN="2" WIDTH="<%= colwidth*2 %>%">Reviewing</th>
<% end %>
<th style="text-align:center;" COLSPAN="2" WIDTH="<%= colwidth*2 %>%">Author Feedback</th>
<% if has_team_and_metareview?[:has_team] %>
  <th style="text-align:center;" COLSPAN="2" WIDTH="<%= colwidth*2 %>%">Teammate Review</th>
<% end %>

  <th style="text-align:center;" WIDTH="<%= colwidth %>%">&nbsp;</th></TR>

<TR <% if prefix %>id='<%= prefix %>_2' style="display:none; background-color: white;"<% end %>>
<th style="text-align:left;" WIDTH="<%= namecolwidth %>%" ALIGN="LEFT">Contributor</th>
<th style="text-align:center;" WIDTH="<%= colwidth %>%">Average</th>
<th style="text-align:center;" WIDTH="<%= colwidth %>%">Range</th>
<% if has_team_and_metareview?[:has_metareview] %>
	<th style="text-align:center;" WIDTH="<%= colwidth %>%">Average</th>
	<th style="text-align:center;" WIDTH="<%= colwidth %>%">Range </th>
<% end %> 
<th style="text-align:center;" WIDTH="<%= colwidth %>%">Average</th>
<th style="text-align:center;" WIDTH="<%= colwidth %>%">Range </th>
  <!--ACS Display teammate review ratings only for teams with size greater than 1-->
<% if has_team_and_metareview?[:has_team] %>
	<th style="text-align:center;" WIDTH="<%= colwidth %>%">Average</th>
	<th style="text-align:center;" WIDTH="<%= colwidth %>%">Range </th>
<% end %>
