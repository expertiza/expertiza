<link rel="stylesheet" type="text/css" href="/assets/timeline.css" >
  <div class="container">
        <div class="row">
        <div class="col-md-12">
          <div class="page-header">
            <h1>Submission History</h1>
          </div>
          <div style="display:inline-block;width:100%;overflow-y:auto;">
            <ul class="timeline timeline-horizontal">
              <% @submission_history.each do |submission| %>
              <%# Display timestamps from each submission! %>
                <li class="timeline-item">
                  <div class="timeline-badge primary"><i class="glyphicon glyphicon-check"></i></div>
                  <div class="timeline-panel">
                    <div class="timeline-heading">
                      <h4 class="timeline-title"><%= submission.type %> <%= submission.action %></h4>
                      <p><small class="text-muted"><i class="glyphicon glyphicon-time"></i> <%= submission.submitted_at %></small></p>
                    </div>
                    <div class="timeline-body">
                      <p><%= submission.submitted_detail %></p>
                    </div>
                  </div>
                </li>
              <% end %>
              <% @assignment.due_dates.each do |due_date| %>
              <%# Display timestamps from each submission! %>
                <li class="timeline-item">
                  <div class="timeline-badge primary"><i class="glyphicon glyphicon-check"></i></div>
                  <div class="timeline-panel">
                    <div class="timeline-heading">
                      <h4 class="timeline-title"> <b>Due Date<b></h4>
                      <p><small class="text-muted"><i class="glyphicon glyphicon-time"></i> <%= due_date.due_at %></small></p>
                    </div>
                    <div class="timeline-body">
                      <p> <%= due_date.deadline_type.name %> Deadline </p>
                    </div>
                  </div>
                </li>
              <% end %>
            </ul>
          </div>
        </div>
      </div>
    </div>